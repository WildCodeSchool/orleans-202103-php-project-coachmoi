{{ form_start(formCoach) }}
<div class="mes-informations border border-dark rounded m-1">
    <div class="profile-h2 border border-dark d-flex align-items-center bg-dark rounded w-75 p-0">
        <h2 class="m-2 text-light text-truncate"><i class="bi bi-heart-fill text-success me-2"></i> Mes préférences</h2>
   </div>
    <div class="m-3">
        <ul>
        {{ form_row(formCoach.activities, {'attr': {'class': 'form-group'}}) }}
        </ul>
        {{ form_row(formCoach.qualification) }}
        {{ form_row(formCoach.hasVehicle) }}
        {{ form_row(formCoach.equipment) }}
        {{ form_row(formCoach.biography) }}
        {{ form_row(formCoach.hourlyRate) }}
    </div>
</div>
<div class="d-flex justify-content-end">
    <button class="btn btn-success text-light mb-5 m-2"><i class="bi bi-check-square-fill text-white"></i> {{ button_label|default('Save') }}</button>
</div>
<div class="mes-informations border border-dark rounded m-1">
    <div class="profile-h2 border border-dark d-flex align-items-center bg-dark rounded w-75 p-0">
        <h2 class="m-2 text-light text-truncate"><i class="bi bi-person-lines-fill text-success me-2"></i> Mes informations</h2>
   </div>
   <div class="m-3">
        {{ form_row(formCoach.user, {'label': false}) }}
        {{ form_row(formCoach.birthdate) }}
        {{ form_row(formCoach.photoFile) }}
    </div>
</div>
<div class="d-flex justify-content-end mb-5">
    <button class="btn btn-success text-light m-2"><i class="bi bi-check-square-fill text-white"></i> {{ button_label|default('Save') }}</button>
</div>
{{ form_end(formCoach) }}